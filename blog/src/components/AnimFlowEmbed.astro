---
import AnimFlowEmbedReact from './AnimFlowEmbed';

interface Props {
  id: string;
  title?: string;
  height?: number;
  yaml: string;
}

const { id, title, height = 400, yaml } = Astro.props;
---

<AnimFlowEmbedReact
  client:only="react"
  id={id}
  title={title}
  height={height}
  yaml={yaml}
/>
